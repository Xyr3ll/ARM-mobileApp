{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Disable React Native Text Watcher",
      "type": "shell",
      "command": "code",
      "args": ["--disable-extension", "react-native-text-watcher"],
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      }
    }
  ]
}
